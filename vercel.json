{
  "version": 2,
  "builds": [
    {
      "src": "dev/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "dev/templates/**",
      "use": "@vercel/static"
    },
    {
      "src": "dev/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dev/$1"
    }
  ]
}
